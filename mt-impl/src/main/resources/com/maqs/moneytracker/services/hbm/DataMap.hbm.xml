<hibernate-mapping>
	
	<class name="com.maqs.moneytracker.model.DataMap" table="BNK_STMNT_DATA_MAP">
		<meta attribute="class-description">This class contains DataMap details.</meta>
		<id name="id" type="long" column="ID">
			<generator class="native" />
		</id>


		<property name="bankStatementId" type="long" column="BNK_STMNT_ID" />
		
		<property name="searchField" type="string" column="SEARCH_PROP_NAME" />
		
		<property name="searchText" type="string" column="SEARCH_TEXT" />
		
		
		<!-- <property name="createdOn" type="timestamp"  column="CREATED_DATE" />
		
		<property name="lastModifiedOn" type="timestamp"  column="MODIFIED_DATE" /> -->
	</class>
	
</hibernate-mapping>